<Window x:Class="sample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:esri="http://schemas.esri.com/arcgis/runtime/2013"
        Title="シンプル アプリ" Height="500" Width="750">
    <Grid>
        <esri:MapView x:Name="MyMapView" Loaded="MyMapView_Loaded" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5">
            <TextBox Width="200" x:Name="addressTextBox"/>
            <Button x:Name="geocoording" Content=" 検索 " Margin="5,0,0,0"  Click="geocoording_Click">
            </Button>
        </StackPanel>
    </Grid>
</Window>
